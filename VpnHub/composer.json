{
    "name": "juan/vpn-zip-downloader",
    "description": "Aplicação",
    "type": "project",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^4.2",
        "doctrine/orm": "^3.3",
        "phpmailer/phpmailer": "^6.10",
        "symfony/cache": "^7.3",
        "symfony/validator": "^7.3",
        "twig/twig": "^3.21",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "scripts": {
      "start-server": "php -S 127.0.0.1:8000",
      "identation": "./vendor/bin/phpcbf --standard=PSR12 src/"
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    
    "minimum-stability": "stable",
    "prefer-stable": true
  }
